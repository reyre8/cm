{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "class-methods-use-this": "off",
    "max-classes-per-file": "off",
    "no-plusplus": "off"
  }
}
